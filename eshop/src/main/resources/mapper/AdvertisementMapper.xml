<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.qingshixun.project.eshop.module.advertisement.dao.AdvertisementDaoMyBatis">

  <select id="getAdvertisements" resultType="AdvertisementDTO">
    SELECT
      ad.id,
      ad.image
    FROM
      qsx_advertisement ad
    WHERE
      ad.isEnable = true
  </select>

</mapper>